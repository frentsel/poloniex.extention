{
  "browser_action": {
    "default_icon": "res/logo16.png",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "libs/jquery-2.1.1.min.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "persistent": false,
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "res/logo128.png",
    "48": "res/logo48.png",
    "16": "res/logo16.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Poloniex",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "proxy",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "assets/*"
  ],
  "version": "0.1"
}
